{
  "name": "botframework-directlinejs",
  "version": "0.10.1-0",
  "description": "client library for the Microsoft Bot Framework Direct Line 3.0 protocol",
  "main": "lib/web/directLine.js",
  "types": "lib/directLine.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "run-p clean build:node:prod build:web:prod emit:node emit:web",
    "build:node:dev": "node ./scripts/build.js",
    "build:web:dev": "node ./scripts/build.js",
    "build:node:prod": "node ./scripts/build.js",
    "build:web:prod": "node ./scripts/build.js",
    "emit:node": "tsc --emitDeclarationOnly --declaration --outdir ./lib/node",
    "emit:web": "tsc --emitDeclarationOnly --declaration --outdir ./lib/web",
    "clean": "rm -rf lib",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://git@github.com/Microsoft/BotFramework-DirectLineJS.git"
  },
  "author": "Microsoft Corp",
  "license": "MIT",
  "dependencies": {
    "botframework-schema": "^4.1.5",
    "form-data": "^2.3.3",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.5",
    "@babel/preset-typescript": "7.1.0",
    "@babel/runtime": "7.1.5",
    "@types/jest": "^22.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "fs-extra": "^7.0.1",
    "jest": "^23.0.0",
    "npm-run-all": "^4.1.3",
    "tslint": "5.11.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.1.6",
    "webpack": "4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(tsx|ts|js)$": "./babel-jest-config"
    },
    "testURL": "http://localhost",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).(ts)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
